@model TestingFinalExam.Models.MathModel


@{
    ViewData["Title"] = "Printable version";
}

<div style="padding: 1rem;">
    <h1>MathLearner</h1>

    <h3>User details</h3>
    <b>First name</b>
    <p>@ViewBag.user.firstname</p>
    <b>Last name</b>
    <p>@ViewBag.user.lastname</p>
    <b>Phone number</b>
    <p>@ViewBag.user.phonenumber</p>
    <b>Age</b>
    <p>@ViewBag.user.age</p>
    <br />
    <h3>MathLearner score</h3>
    <b>Predicted score</b>
    <p>@ViewBag.user.guessScore</p>
    <b>Actual score</b>
    <p>@ViewBag.score</p>
</div>